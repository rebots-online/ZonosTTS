{
  "name": "zonos-tts-pwa",
  "version": "1.0.0",
  "description": "ZonosTTS Progressive Web App with Android support",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "webpack --config config/production.config.js",
    "build:android": "cd android && ./gradlew assembleRelease",
    "build:web": "npm run build && npm run optimize",
    "optimize": "node scripts/optimize-assets.js",
    "analyze": "webpack-bundle-analyzer stats.json",
    "prepare-release": "node scripts/prepare-release.js",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,scss,md}\"",
    "type-check": "tsc --noEmit",
    "deps:cache": "node scripts/manage-deps.js cache",
    "deps:install": "node scripts/manage-deps.js install",
    "deps:status": "node scripts/manage-deps.js status",
    "postinstall": "npm run deps:cache",
    "prepare": "husky install"
  },
  "dependencies": {
    "@chargebee/chargebee-js": "^3.9.1",
    "@react-native-google-signin/google-signin": "^10.1.1",
    "@react-native-community/masked-view": "0.1.11",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/stack": "^6.3.20",
    "buffer": "^6.0.3",
    "compression-webpack-plugin": "^10.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "crypto-browserify": "^3.12.0",
    "onnxruntime-web": "^1.17.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-native": "^0.73.0",
    "react-native-billing": "^3.0.1",
    "react-native-web": "^0.19.0",
    "react-scripts": "5.0.1",
    "stream-browserify": "^3.0.0",
    "terser-webpack-plugin": "^5.3.9",
    "typescript": "^5.3.0",
    "workbox-webpack-plugin": "^7.0.0",
    "@babel/plugin-proposal-decorators": "^7.23.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "metro-react-native-babel-preset": "^0.77.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/react-native": "^0.73.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-react": "^7.33.0",
    "prettier": "^3.0.0",
    "@react-native/metro-config": "^0.73.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "@react-native/eslint-config": "^0.73.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
